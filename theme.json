{
  "$schema": "https://schemas.wp.org/wp/6.1/theme.json",
  "version": 2,
  "customTemplates": [],
  "settings": {
    "border": {
      "radius": false,
      "color": false,
      "style": false,
      "width": false
    },
    "color": {
      "custom": false,
      "customDuotone": false,
      "customGradient": false,
      "duotone": [],
      "link": false,
      "text": true,
      "background": false,
      "defaultGradients": false,
      "defaultPalette": false,
      "gradients": [],
      "palette": [
        {
          "slug": "foreground",
          "color": "#000000",
          "name": "Foreground"
        },
        {
          "slug": "background",
          "color": "#ffffff",
          "name": "Background"
        },
        {
          "slug": "accent",
          "color": "#b0Bc67",
          "name": "Accent"
        },
        {
          "slug": "secondary",
          "color": "#450B40",
          "name": "Secondary"
        }
      ]
    },
    "custom": {
      "spacing": {
        "small": "max(1.25rem, 5vw)",
        "medium": "clamp(2rem, 8vw, calc(4 * var(--wp--style--block-gap)))",
        "large": "clamp(4rem, 10vw, 8rem)",
        "outer": "var(--wp--custom--spacing--small, 1.25rem)"
      },
      "typography": {
        "font-size": {
          "btn": "1.2em",
          "h1": "clamp(2.25rem, 4vw, 5rem)",
          "h2": "clamp(2rem, 3vw, 3.125rem)",
          "h3": "clamp(1.8rem, 2vw, 2.5rem)"
        },
        "line-height": {
          "tiny": 1.15,
          "small": 1.2,
          "medium": 1.4,
          "normal": 1.6
        }
      }
    },
    "spacing": {
      "margin": false,
      "padding": false,
      "blockGap": true,
      "units": ["%", "px", "em", "rem", "vh", "vw"]
    },
    "typography": {
      "lineHeight": false,
      "customFontSize": false,
      "dropCap": false,
      "fontStyle": false,
      "fontWeight": false,
      "letterSpacing": false,
      "textDecoration": false,
      "textTransform": true,
      "fontFamilies": [
        {
          "fontFamily": "\"Roboto\", sans-serif",
          "name": "Roboto",
          "slug": "Roboto",
          "fontFace": [
            {
              "fontFamily": "Roboto",
              "fontWeight": "400",
              "fontStyle": "normal",
              "fontStretch": "normal",
              "src": ["file:./assets/fonts/roboto-v27-latin-regular.woff2"]
            },
            {
              "fontFamily": "Roboto",
              "fontWeight": "400",
              "fontStyle": "italic",
              "fontStretch": "normal",
              "src": ["file:./assets/fonts/roboto-v27-latin-italic.woff2"]
            },
            {
              "fontFamily": "Roboto",
              "fontWeight": "700",
              "fontStyle": "normal",
              "fontStretch": "normal",
              "src": ["file:./assets/fonts/roboto-v27-latin-700.woff2"]
            }
          ]
        },
        {
          "fontFamily": "\"Norican\", sans-serif",
          "name": "Norican",
          "slug": "Norican",
          "fontFace": [
            {
              "fontFamily": "Norican",
              "fontWeight": "400",
              "fontStyle": "normal",
              "fontStretch": "normal",
              "src": ["file:./assets/fonts/Norican-Regular.woff2"]
            }
          ]
        }
      ],
      "fontSizes": [
        {
          "size": "0.9rem",
          "slug": "small"
        },
        {
          "size": "1rem",
          "slug": "medium"
        },
        {
          "size": "1.125rem",
          "slug": "large"
        },
        {
          "size": "1.75rem",
          "slug": "x-large"
        },
        {
          "size": "clamp(1.75rem, 3vw, 2.25rem)",
          "slug": "xx-large"
        }
      ]
    },
    "layout": {
      "contentSize": "90%",
      "wideSize": "98%"
    }
  },
  "styles": {
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--roboto)",
      "lineHeight": "var(--wp--custom--typography--line-height--normal)",
      "fontSize": "var(--wp--preset--font-size--medium)"
    },
    "blocks": {
      "core/button": {
        "border": {
          "radius": "0",
          "style": "solid",
          "width": "1px",
          "color": "var(--wp--preset--color--secondary)"
        },
        "spacing": {
          "padding": {
            "bottom": ".8rem",
            "top": ".8rem"
          }
        },
        "color": {
          "background": "var(--wp--preset--color--accent)",
          "text": "var(--wp--preset--color--background)"
        },
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--roboto)",
          "fontWeight": "600",
          "fontSize": "var(--wp--custom--typography--font-size--btn)",
          "textTransform": "uppercase"
        }
      },
      "core/post-title": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--roboto)",
          "fontWeight": "600",
          "lineHeight": "var(--wp--custom--typography--line-height--tiny)",
          "fontSize": "var(--wp--custom--typography--font-size--h-1)"
        }
      },
      "core/pullquote": {
        "border": {
          "width": "1px 0"
        }
      },
      "core/quote": {
        "border": {
          "width": "1px"
        }
      },
      "core/site-title": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--roboto)",
          "lineHeight": "var(--wp--custom--typography--line-height--normal)",
          "fontSize": "var(--wp--preset--font-size--medium)",
          "fontStyle": "italic",
          "fontWeight": "normal"
        }
      }
    },
    "elements": {
      "h1": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--norican)",
          "fontWeight": "700",
          "lineHeight": "var(--wp--custom--typography--line-height--tiny)",
          "fontSize": "var(--wp--custom--typography--font-size--h-1)"
        }
      },
      "h2": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--norican)",
          "fontWeight": "700",
          "lineHeight": "var(--wp--custom--typography--line-height--small)",
          "fontSize": "var(--wp--custom--typography--font-size--h-2)"
        }
      },
      "h3": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--norican)",
          "fontWeight": "700",
          "lineHeight": "var(--wp--custom--typography--line-height--tiny)",
          "fontSize": "var(--wp--custom--typography--font-size--h-3)"
        }
      },
      "h4": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--norican)",
          "fontWeight": "700",
          "lineHeight": "var(--wp--custom--typography--line-height--tiny)",
          "fontSize": "var(--wp--preset--font-size--x-large)"
        }
      },
      "h5": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--norican)",
          "fontWeight": "700",
          "textTransform": "uppercase",
          "lineHeight": "var(--wp--custom--typography--line-height--normal)",
          "fontSize": "var(--wp--preset--font-size--medium)"
        }
      },
      "h6": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--norican)",
          "fontWeight": "700",
          "textTransform": "uppercase",
          "lineHeight": "var(--wp--custom--typography--line-height--normal)",
          "fontSize": "var(--wp--preset--font-size--medium)"
        }
      },
      "link": {
        "color": {
          "text": "var(--wp--preset--color--accent)"
        }
      }
    }
  },
  "templateParts": []
}
